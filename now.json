{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "/404" }
  ],
  "build": {
    "env": {
      "GOOGLE_MAPS_API": "@njab-map-api"
    }
  }
}
